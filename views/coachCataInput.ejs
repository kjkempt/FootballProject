    <div id="page-wrapper">

        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <span class = "text-nowrap">
                                    <h1 class="page-header" >
                                        Input Catapult Data
                                    </h1>
                                    </span>
                    <div class="panel panel-default">

                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="fa fa-bar-chart-o fa-fw"></i> Data Entry</h3>
                        </div>
                        <div class = "panel-body">
                            <form name="dataEntry"  class = "form-inline" action="/coachCataInput/entry" method="post" role="form"
                                  style=" display: block;" onsubmit="return validateForm()">
                                <div class="form-group">
                                    <select class="form-control" id="mySelect" name="player">
                                        <option disabled selected value="default"> -- Select a player -- </option>
                                        <% for( j = 0; j < player.length; j++){  %>
                                        <tr>
                                            <option value = "<%= player[j].username %>" ><%= player[j].last_name %>, <%= player[j].first_name%></option>
                                        </tr>
                                        <%} %>
                                    </select>
                                    <input type="text" name = "workoutLoad"
                                           onkeypress='return event.charCode >= 48 && event.charCode <= 57'
                                           class="form-inline" placeholder="Load">
                                    </input>
                                    <input type="text" name = "workoutDuration"
                                           onkeypress='return event.charCode >= 48 && event.charCode <= 57'
                                           class="form-inline" placeholder="Duration (Min.)">
                                    </input>
                                </div>
                                <button type="submit" class="btn btn-default">Submit</button>
                                <p class = "text-success" style="display: inline"><%= message %></p>
                            </form>


                        </div>



                    </div>
                </div>
