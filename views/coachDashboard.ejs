    <div id="page-wrapper">

        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="fa fa-bar-chart-o fa-fw"></i> Individual Player Analysis</h3>
                        </div>
                        <div class = "panel-body">


                            <table id="player_table" class="display">
                                <thead>
                                <tr>

                                    <th>First Name</th>
                                    <th>Last Name</th>
                                    <th>Postion</th>
                                    <th>sRPE</th>
                                    <th>Duration</th>
                                    <th>Acute Load</th>
                                    <th>Rolling Load (Chronic - 28 day)</th>
                                </tr>
                                </thead>

                                <tbody>

                                <% player_data.forEach(function (data) {
                                    var chronicLoad = data.rpeSUM * data.durationSUM; %>

                                <% one_week_data.forEach(function (oneweek) {
                                    var acuteLoad = oneweek.rpeSUM * oneweek.durationSUM; %>

                                <% if (oneweek.username == data.username){%>
                                <tr>
                                    <!-- Multiply sum of player RPE and duration for chronic load -->



                                    <td> <%= data.last_name %></td>
                                    <td><%= data.first_name%></td>
                                    <td><%= data.position%></td>
                                    <td><%= data.rpeSUM%></td>
                                    <td><%= data.durationSUM%></td>
                                    <td><%=acuteLoad%></td>
                                    <td><%= chronicLoad %> </td>






                                </tr>
                                <%}%>

                                <% }) %>
                                <% }) %>
                                </tbody>
                            </table>
                            <script>
                                var $ = jQuery;
                                $(document).ready( function () {
                                    $('#player_table').DataTable();
                                } );


                            </script>
                        </div>
                        <div class="panel-body">
                            <div id="morris-area-chart"></div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.row -->

        </div>
        <!-- /.container-fluid -->

